{
	"name": "traction-front",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"antd": "^4.23.4",
		"axios": "^1.1.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router": "^6.4.2",
		"react-router-dom": "^6.4.2",
		"react-scripts": "^4.0.3",
		"styled-components": "^5.3.6",
		"swiper": "^8.4.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		],
		"rules": {
			"react-hooks/exhaustive-deps": "off",
			"react-hooks/rules-of-hooks": "off",
			"indent": [
				"error",
				"tab"
			],
			"linebreak-style": 0,
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"comma-spacing": [
				"error",
				{
					"before": false,
					"after": true
				}
			],
			"semi-spacing": [
				"error",
				{
					"before": false,
					"after": true
				}
			],
			"object-curly-spacing": [
				"error",
				"always"
			],
			"key-spacing": [
				"error",
				{
					"beforeColon": false
				}
			],
			"padded-blocks": [
				"error",
				"never"
			],
			"no-multiple-empty-lines": [
				"error",
				{
					"max": 1
				}
			],
			"lines-between-class-members": [
				"error",
				"always",
				{
					"exceptAfterSingleLine": true
				}
			],
			"space-before-blocks": [
				"error",
				"always"
			],
			"eol-last": [
				"error",
				"always"
			],
			"arrow-spacing": [
				"error",
				{
					"before": true,
					"after": true
				}
			],
			"no-console": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
